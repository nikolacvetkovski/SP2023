int main()
{
    int array[1000], target;
    
    for(int i = 0; i < 1000; i++) {
        array[i] = i + 1;
    }
    
    for(int i = 0; i < 1000; i++) {
        cout << array[i] << " ";
    }
    
    cout << endl;
    
    cin >> target;
    
    cout << linearSearch(array, target);
    
    cout << endl;
    
    cout << binarySearch(array, 0, 1000, target);
    
    return 0;
}
