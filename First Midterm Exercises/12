#include <iostream>
using namespace std;

int main() {
    int number, maxDigit = 0, maxPos, currPos = 0, pos0, pos1, pos2, pos3, pos4;
    pos0 = pos1 = pos2 = pos3 = pos4 = 0;
    
    while(cin >> number) {
        maxDigit = 0, currPos = 0;
        int temp = number;
        while(temp > 0) {
            int digit = temp % 10;
            if(digit > maxDigit) {
                maxDigit = digit;
                maxPos = currPos;
            }
            ++currPos;
            temp /= 10;
        }
        
        if(maxPos == 0) {
            ++pos0;
        }
        else if(maxPos == 1) {
            ++pos1;
        }
        else if(maxPos == 2) {
            ++pos2;
        }
        else if(maxPos == 3) {
            ++pos3;
        }
        else if(maxPos == 4) {
            ++pos4;
        }
    }
    
    cout << "0: " << pos0 << endl << "1: " << pos1 << endl << "2: " << pos2 << endl;
    cout << "3: " << pos3 << endl << "4: " << pos4 << endl;

    return 0;
}
