#include <iostream>
using namespace std;

int main() {
    int n, i = 0, number, maxNumber = 0;
    cin >> n;
    
    while(i < n) {
        cin >> number;
        int sum = 0, temp = number;
        
        while(temp > 0) {
            int digit = temp % 10;
            sum += digit;
            temp /= 10;
        }
        
        if(sum % 2 == 0) {
            if(number > maxNumber) {
                maxNumber = number;
            }
        }
        
        ++i;
    }
    cout << maxNumber;

    return 0;
}
