#include <iostream>
using namespace std;

bool isPrimeNumber(int number) {
    bool isTrue = false;
    for(int j = number - 1; j > 1; j--) {
        if(number % j != 0) {
            isTrue = true;
        }
        else {
            isTrue = false;
            break;
        }
    }
    if(!isTrue) {
        return false;
    }
    else {
        return true;
    }
}

int main() {
   int number, biggerNumber;
   cin >> number;
   
   for(int i = number + 1; i > 0; i++) {
       if(isPrimeNumber(i) == true) {
           biggerNumber = i;
           break;
       }
   }
   cout << biggerNumber << " - " << number << " = " << biggerNumber - number << endl;
   return 0;
}
