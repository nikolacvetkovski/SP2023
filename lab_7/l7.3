#include <iostream>
using namespace std;

int main()
{
    int array[100][100], n;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
       for(int j = 0; j < n; j++) {
           cin >> array[i][j];
       }
    }
    
    for(int i = 0; i < n; i++) {
       for(int j = 0; j < n; j++) {
           if(i > j && i + j >= n) {
               array[i][j] *= -1;
           }
       }
    }
    
    for(int i = 0; i < n; i++) {
       for(int j = 0; j < n; j++) {
          cout << array[i][j] << " ";
       }
       cout << endl;
    }
    
    return 0;
}
