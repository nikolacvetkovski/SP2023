#include <iostream>
using namespace std;

int main()
{
    int array[100][100], n, m;
    cin >> n;
    
    m = n * 2;
    for(int i = 0; i < n; i++) {
       for(int j = 0; j < m; j++) {
           cin >> array[i][j];
       }
    }
    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m / 2; j++) {
            cout << array[i][j] << " ";
        }
        cout << endl;
    }
    
    for(int i = 0; i < n; i++) {
        for(int j = m / 2; j < m; j++) {
            cout << array[i][j] << " ";
        }
        cout << endl;
    }
    
    return 0;
}
