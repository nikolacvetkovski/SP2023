#include <iostream>
using namespace std;

int main()
{
    int array[100][100], n, sum = 0, prevsum = 0;
    bool isTrue;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
       for(int j = 0; j < n; j++) {
           cin >> array[i][j];
       }
    }
    
    for(int i = 0; i < n; i++) {
        prevsum = sum, sum = 0;
       for(int j = 0; j < n; j++) {
           sum += array[i][j];
       }
       
       if(i == 0) {
           prevsum = sum;
       }
       
       if(sum == prevsum) {
           isTrue = true;
       } else {
           isTrue = false;
           break;
       }
    }
    
    if(isTrue) {
        cout << "MAGIC SQUARE!" << endl;
        for(int i = 0; i < n; i++) {
           for(int j = 0; j < n; j++) {
              if(i == j) {
                  array[i][j] = sum;
              }
              if(i + j == n - 1) {
                  array[i][j] = sum;
              }
           }
        }
        
         for(int i = 0; i < n; i++) {
           for(int j = 0; j < n; j++) {
              cout << array[i][j] << " ";
           }
           cout << endl;
        }
    }
    else {
        cout << "NOT a Magic Square!" << endl;
    }
    
    return 0;
}
