#include <iostream>
using namespace std;

int main() {
    int num1, num2, num3 ,num4;
    cin >> num1 >> num2 >> num3 >> num4;

    int sum1 = num1 + num2;
    int sum2 = num3 + num4;

    if(sum1 % 2 == 0 && sum2 % 2 != 0) {
        cout << "1" << endl;
    }
    else {
        cout << "0" << endl;
    }
    return 0;
}
