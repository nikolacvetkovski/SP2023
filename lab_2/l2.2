#include <iostream>
using namespace std;

int main() { 
int sixDigitNumber;
    cin >> sixDigitNumber;
    
    if((sixDigitNumber / 100000) == (sixDigitNumber % 10)) {
        if((sixDigitNumber / 10000 % 10) == (sixDigitNumber / 10 % 10)) {
            if((sixDigitNumber / 1000 % 10) == (sixDigitNumber / 100 % 10)) {
                cout << "1";
            }
        }
    }
    else {
        cout << "0";
    }
    return 0;
}
