#include <iostream>
using namespace std;

int main() {
   int array[100], n;
   cin >> n;
   
   for(int i = 0; i < n; i++) {
       cin >> array[i];
   }
   
   int sum = 0;
   for(int i = 0; i < n; i++) {
       sum += array[i];
   }
   double average = (double) sum / n;
   
   for(int i = 0; i < n; i++) {
       if(array[i] >= average) {
           array[i] = 1;
       }
       else {
           array[i] = 0;
       }
   }
   
   for(int i = 0; i < n; i++) {
       cout << array[i] << " ";
   }
   
   return 0;
}
