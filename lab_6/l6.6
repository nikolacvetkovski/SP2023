#include <iostream>
using namespace std;

int main()
{
    int array[100], n, k;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> array[i];
    }
    
    cin >> k;
    
    for(int i = 0; i < n; i++) {
        if(k == i) {
            int temp1 = array[i];
            array[i] = array[i - 1];
            array[i - 1] = temp1;
            
            int temp2 = array[n - k - 1];
            array[n - k - 1] = array[n - k - 2];
            array[n - k - 2] = temp2;
        }
    }
    
    for(int i = 0; i < n; i++) {
        cout << array[i] << " ";
    }

    return 0;
}
