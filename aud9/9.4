#include <iostream>
using namespace std;

float xnn(int number) {
    if(number == 1 || number == 2) {
        return number;
    }
    else {
        // x[n] = (n - 1) * x[n - 1] / n + x[n - 2] / n
        return (number - 1) * xnn(number - 1.0) / number + xnn(number - 2.0) / number;
    }
}

int main() 
{
    int n;
    cin >> n;
    
    cout << xnn(n) << endl;
    return 0;
}
