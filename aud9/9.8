#include <iostream>
using namespace std;

int LCM(int m, int n, int divisor) { 
    if (m == 1 && n == 1) {
        return 1;
    }
    
    if (m % divisor == 0 || n % divisor == 0) {
        if(m % divisor == 0) {
            m /= divisor;
        }
            
        if(n % divisor == 0) {
            n /= divisor;
        }
            
        return divisor * LCM(m , n, divisor);
    }
        
    else {
        return LCM(m, n, divisor + 1);
    }
}

int main() 
{
    int array[100], n;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> array[i];
    }
    
    int lcm = LCM(array[0], array[1], 2);
    for(int i = 2; i < n; i++) {
        lcm = LCM(lcm, array[i], 2);
    }
    cout << lcm << endl;
    
    return 0;
}
