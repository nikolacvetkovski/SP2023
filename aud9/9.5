#include <iostream>
using namespace std;

int sumOfDigits(int number) {  // 956 ---> 20 ; 6 + 95; posl. cifra + povikuvacka funckija za ostat. od brojot bez posl.cifra;
    if(number == 0) {
        return 0;
    }
    else {
        return number % 10 + sumOfDigits(number / 10);
    }
}

int main() 
{
    int number;
    cin >> number;
    
    cout << sumOfDigits(number) << endl;
    return 0;
}
