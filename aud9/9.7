#include <iostream>
using namespace std;

int GCD(int m, int n) {
    if(n == 0) {
        return m;
    }
    return GCD(n, m % n);   // 12, 8 -->  8, 4 --> 4, 0
}

int main() 
{
    int array[100], n;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> array[i];
    }
    
    int gcd = GCD(array[0], array[1]);
    for(int i = 2; i < n; i++) {
        gcd = GCD(gcd, array[i]);
    }
    
    cout << gcd << endl;
    
    return 0;
}
