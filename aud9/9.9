#include <iostream>
using namespace std;

int sumOfElements(int array[], int n) {   // 6 5 8 4
    if(n == 1) {
       return array[0];
    }
    else {
        return array[n - 1] + sumOfElements(array, n - 1);  // 4 + 8 + 5 + 6
    }
}

int main() 
{
    int array[100], n;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> array[i];
    }
    
    cout << sumOfElements(array, n);
    return 0;
}
