#include <iostream>
using namespace std;

void premesti(int a[], int n) {
    int merged[n], j = 0;
    for(int i = 0; i < n; i++) {
        if(a[i] >= 0) {
            merged[j++] = a[i];
        }
    }
    
    for(int i = 0; i < n; i++) {
        if(a[i] < 0) {
            merged[j++] = a[i];
        }
    }
    
    for(int i = 0; i < n; i++) {
        a[i] = merged[i];
    }
}

int main() {
    int a[100], n;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> a[i];
    }
    
    premesti(a, n);
    
    for(int i = 0; i < n; i++) {
        cout << a[i] << " ";
    }
    
    return 0;
}
