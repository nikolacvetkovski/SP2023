#include <iostream>
using namespace std;

int main() {
  int n, number, div = 0, mod1 = 0, mod2 = 0;
    cin >> n;

    for(int i=0; i<n; i++) {
        cin >> number;
        if(number % 3 == 0) {
            div++;
        }
        else if(number % 3 == 1) {
            mod1++;
        }
        else if(number % 3 == 2) {
            mod2++;
        }
    }
}
