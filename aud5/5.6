#include <iostream>
using namespace std;

int main()
{
    int number, maxValue, maxPos, counter = 1;
    
    while(cin >> number) {
        if(number > maxValue) {
            maxValue = number;
            maxPos = counter;
        }
        ++counter;
    }
    
    cout << "Maximum number is: " << maxValue << " which was entered " << maxPos << " in a row";
    cout << " out of " << counter - 1 << " entered numbers!" << endl;
    return 0;
}
