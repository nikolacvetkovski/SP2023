void transform(char *arr, char *sub) {
    int len1 = strlen(sub), len2 = strlen(arr), j = 0, pos;
    for(int i = 0; arr[i] != '\0'; i++) {
        if(strncmp(sub, arr + i, len1) == 0) {
            pos = i;
        }
    }
    
    for(int i = 0; i < pos; i++) {
        arr[j++] = arr[i];
    }
    for(int i = pos + len1; arr[i] != '\0'; i++) {
        arr[j++] = arr[i];
    }
    arr[j] = '\0';
}

int main()
{
    char arr[100], subarr[100];
    cin.getline(arr, 100);
    cin.getline(subarr, 100);
    
    transform(arr, subarr);
    
    cout << arr << endl;
    
    return 0;
}
