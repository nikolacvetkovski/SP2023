#include <iostream>
#include <cstring>
#include <cctype>
using namespace std;  //sakAm da KAZHAm deka te sAkam najmnogu haha!

int letterFrequency(char *text, char letter) {
    int counter = 0, s;
    s = strlen(text);
    for(int i = 0; text[i] != '\0'; i++) {
        if(tolower(letter) == tolower(text[i])) {
            ++counter;
        }
    }
    return ((float)counter / s) * 100;
}

int main()
{
    char text[100], letter;
    cin.getline(text, 100);
    cin >> letter;
    
    int result = letterFrequency(text, letter);
    cout << "Relative Frequeny of the letter " << "'" << letter << "'" << " is " << result << "%" <<endl;
    
    return 0;
}
