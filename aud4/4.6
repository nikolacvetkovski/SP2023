#include <iostream>
using namespace std;

int main()
{
    int dogAge, humanAge;
    cin >> dogAge;

    if(dogAge < 0) {
        cout << "Age must be a positive number!" << endl;
        return 0;
    }
    else {
        if(dogAge <= 2) {
            humanAge = dogAge * 10.5;
        }
        else {
            humanAge = 2 * 10.5 + (dogAge - 2) * 4;
        }
        cout << "Human Age of Dog's Age is: " << humanAge << endl;
    }
    return 0;
}
