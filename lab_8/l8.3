#include <iostream>
using namespace std;

bool isZigZag(int number, int prev, int direction) {
    if (number == 0) {
        return 1;
    }
    
    int curr = number % 10;
    if (prev != -1) {
        if (direction == 0 && curr >= prev) {
            return 0;
        } 
        else if (direction == 1 && curr <= prev) {
            return 0;
        }
    }
    if(prev == -1) {
        prev = -1;
    }
    else {
        if(curr > prev) {
            prev = 0;
        }
        else {
            prev = 1;
        }
    }
    return isZigZag(number / 10, curr, prev);
}

int main()
{
    int number;
    cin >> number;
    
    if(isZigZag(number, -1, -1)) {
        cout << "ZigZag number!" << endl;
    }
    else {
        cout << "Not a ZigZag number!" << endl;
    }
    
    return 0;
}
