#include <iostream>
using namespace std;

int numberOfTimes(int array[], int n, int k, int i) {
    if(i == n - 1) {
        if(array[i] == k) {
            return 1;
        }
        else {
            return 0;
        }
    }
    
    if(array[i] == k) {
        return 1 + numberOfTimes(array, n, k, i + 1);
    }
    else {
        return numberOfTimes(array, n, k, i + 1);
    }
}

int main()
{
    int arr[100], n, k;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    
    cin >> k;
    
    int result = numberOfTimes(arr, n, k, 0);
    cout << result << endl;
    
    return 0;
}
