#include <iostream>
using namespace std;

bool isAscending(int x) {
    if(x == 1) {
        return true;
    }
    
    if(x % 10 > x / 10 % 10) {
        return isAscending(x / 10);
    }
    else {
        return false;
    }
}

int main()
{
    int n, x, i = 0;
    cin >> n;
    
    while(i < n) {
        cin >> x;
        cout << isAscending(x) << endl;
        ++i;
    }

    return 0;
}
