#include <iostream>
using namespace std;

void mergedArray(int arr1[], int arr2[], int m, int n, int arr3[]) {
    int i = 0, j = 0, k = 0;
    while(i < m && j < n) {
        if(arr1[i] < arr2[j]) {
            arr3[k++] = arr1[i++];
        }
        else {
            arr3[k++] = arr2[j++];
        }
    }
    
    while(i < m) {
        arr3[k++] = arr1[i++];
    }
    
    while(j < n) {
        arr3[k++] = arr2[j++];
    }
}

int main()
{
    int arr1[100], arr2[100], m, n;
    
    cin >> m;
    for(int i = 0; i < m; i++) {
        cin >> arr1[i];
    }
    
    cin >> n;
    for(int j = 0; j < n; j++) {
        cin >> arr2[j];
    }
    
    int newSize = m + n;
    int arr3[newSize];
    mergedArray(arr1, arr2, m, n, arr3);
    
    for(int i = 0; i < newSize; i++) {
        cout << arr3[i] << " ";
    }
    
    return 0;
}
