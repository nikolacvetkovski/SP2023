#include <iostream>
#include <cstring>
using namespace std;

int main()
{
    char row1[100], row2[100], row3[100];
    int n, k = 0, counter;
    cin.getline(row1, 100), cin.getline(row2, 100);
    
    n = strlen(row1);
    for(int i = 0; i < n; i++) {
        counter = 0;
        if(row1[i] == row2[i]) {
            row3[k++] = '*';
        }
        else {
            for(int j = 0; j < n; j++) {
                if(row1[i] == row2[j]) {
                    ++counter;
                }
            }
            
            if(counter > 9) {
                row3[k++] = 'X';
            }
            else {
                row3[k++] = counter + '0';
            }
        }
    }
    row3[k] = '\0';
    
    cout << row3 << endl;
    return 0;
}
