#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int m[100][100], n, sum;
    bool isSquare = false;
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            cin >> m[i][j];
        }
    }
    
    for(int i = 0; i < n; i++) {
        sum = 0;
        for(int j = 0; j < n; j++) {
            if(i == j) {
                sum += m[i][j];
            }
            if(i + j == n - 1) {
                sum += m[i][j];
            }
        }
        if(sqrt(sum) == floor(sqrt(sum))) {
            isSquare = true;
        }
        else {
            isSquare = false;
            break;
        }
    }
    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            cout << m[i][j] << " ";
        }
        cout << endl;
    }
    
    !isSquare ? cout << "NO" : cout << "YES";
    return 0;
}
