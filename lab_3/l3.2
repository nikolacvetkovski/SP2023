#include <iostream>
using namespace std;

int main() {
    int sixDigitNumber;
    cin >> sixDigitNumber;

    int firstDigit = sixDigitNumber / 100000; // 123456
    int secondDigit = sixDigitNumber / 10000 % 10;
    int thirdDigit = sixDigitNumber / 1000 % 10;
    int fourthDigit =sixDigitNumber / 100 % 10;
    int fifthDigit = sixDigitNumber / 10 % 10;
    int sixthDigit = sixDigitNumber % 10;

    if(firstDigit == sixthDigit && secondDigit == fifthDigit && thirdDigit == fourthDigit) {
        cout << "1";
    }
    else {
        cout << "0";
    }
    return 0;
}
