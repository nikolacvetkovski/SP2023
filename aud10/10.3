#include <iostream>
using namespace std;

int counterLetter(char *array, char letter) {
    int counter = 0;
    for(int i = 0; *(array + i) != '\0'; i++) {
        if(*(array + i) == letter) {
            ++counter;
        }
    }
    
    return counter;
}

int main()
{
    char text[100], letter;
    int counter = 0;
    cin.getline(text, 100);
    cin >> letter;
    
    cout << counterLetter(text, letter);
    
    return 0;
}
