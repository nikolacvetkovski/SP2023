#include <iostream>
#include <cstring>
#include <cctype>
using namespace std;

void transformString(char *text) {
    char modify[100];
    int j = 0;
    for(int i = 0; text[i] != '\0'; i++) {
        if(!isdigit(text[i]) && !ispunct(text[i])) {
            if(isupper(text[i])) {
                modify[j++] = tolower(text[i]);
            }
            else if(islower(text[i])) {
                modify[j++] = toupper(text[i]);
            }
        }
    }
    modify[j] = '\0';
    
    for(int i = 0; modify[i] != '\0'; i++) {
        text[i] = modify[i];
    }
    text[j] = '\0';
}

int main()
{
    char text[100];
    cin.getline(text, 100);
    
    transformString(text);
    
    cout << text << endl;
    
    return 0;
}
