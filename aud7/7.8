#include <iostream>
using namespace std;

int main()
{
    int array[100][100], n, m, sumOdd = 0, sumEven = 0;
    cin >> n >> m;
    
    for(int i=0; i<n; i++) {
        for(int j=0; j<m; j++) {
            cin >> array[i][j];
        }
    }
    
    for(int i=0; i<n; i++) { 
        for(int j=0; j<m; j++) {
            if(i % 2 == 0) {
                sumEven += array[i][j];
            }
            if(j % 2 != 0) {
                sumOdd += array[i][j];
            }
        }
    }
    
    cout << sumOdd << endl << sumEven << endl;
    cout << sumOdd - sumEven << endl;

    return 0;
}
