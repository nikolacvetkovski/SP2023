#include <iostream>
using namespace std;

int main() {
  int array1[100], array2[100], n, m;
  bool isTrue = false;
  cin >> n >> m;
  
  if(n != m) {
      cout << "The arrays are different!" << endl;
      return 0;
  }
  
  for(int i = 0; i < n; i++) {
      cin >> array1[i];
  }
  
  for(int i = 0; i < n; i++) {
      cin >> array2[i];
  }
  
  for(int i = 0; i < n; i++) {
      if(array1[i] != array2[i]) {
          isTrue = false;
          break;
      }
      else {
          isTrue = true;
      }
  }
  
  if(!isTrue) {
      cout << "The arrays are different!" << endl;
  }
  else {
      cout << "The arrays are similar!" << endl;
  }
  return 0;
}
